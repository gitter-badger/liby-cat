{
  "name": "catalog",
  "plural": "catalog",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "catalogIndex": {
    "keys": {
      "orgIdx": 1,
      "catalogIdx": 1
    },
    "options": {
      "unique": true
    }
  },
  "properties": {
    "orgIdx": {
      "type": "string",
      "required": true
    },
    "catalogIdx": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true,
      "default": "books"
    }
  },
  "validations": [],
  "relations": {
    "org": {
      "type": "belongsTo",
      "model": "org",
      "foreignKey": ""
    },
    "entries": {
      "type": "hasMany",
      "model": "entry",
      "foreignKey": ""
    },
    "owners": {
      "type": "hasAndBelongsToMany",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}
